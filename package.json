{
  "name": "zev-tiny-observable",
  "version": "0.1.0",
  "description": "tiny-observable",
  "main": "dist/TinyObservable.js",
  "private": false,
  "source": "src/TinyObservable.js",
  "module": "dist/TinyObservable.js",
  "scripts": {
    "build": "rollup ./src/TinyObservable.js --file dist/TinyObservable.js --format iife --name 'TinyObservable' && terser dist/TinyObservable.js --compress --mangle --output dist/TinyObservable.min.js",
    "dev": "rollup ./src/TinyObservable.js --file dist/TinyObservable.js --format iife --name 'TinyObservable' --watch",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phillyDesignr/tiny-observable.git"
  },
  "author": "Jeremy Zevin",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/phillyDesignr/tiny-observable/issues"
  },
  "homepage": "https://github.com/phillyDesignr/tiny-observable#readme",
  "devDependencies": {
    "rollup": "^1.16.2",
    "terser": "^4.0.0"
  },
  "dependencies": {}
}
